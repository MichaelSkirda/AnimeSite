@model AnimeSite.Models.ViewModels.YearsViewModel


@section Title
{
    <title>Поиск по годам - anifox.ru</title>
}

@section Styles
{
    <link rel="stylesheet" href="~/css/years.css">
}


<div class="content-list" id="posts-wrap">


    @for(int i = Model.FromYear; i < Model.ToYear; i++)
    {
        <div class="content-block">
            
            <a href="~/year/@(i)">
                <div class="content-img-wrap">
                  <span>@(i)</span>
                </div>
            </a>
        </div>
    }



</div>
